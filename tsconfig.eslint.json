{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "paths": {
      "src/*": [
        "./*"
      ],
      "src": [
        "."
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    "types/**/*.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
